{
  "name": "aladdin-v3-contracts",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "test": "forge test && yarn hardhat test",
    "testhh": "yarn hardhat test --no-compile",
    "build": "yarn hardhat compile",
    "votium:vote": "yarn ts-node scripts/votium/hermez_vote.ts",
    "votium:snapshot_vote": "yarn ts-node scripts/votium/snapshot_vote.ts",
    "votium:proof": "yarn ts-node scripts/votium/get_proof.ts --round",
    "votium:harvest": "env HARDHAT_NETWORK=mainnet yarn ts-node scripts/votium/harvest.ts --round",
    "sdcrv:harvest": "env HARDHAT_NETWORK=mainnet yarn ts-node scripts/stakedao/harvest.ts --round",
    "cvxcrv:weight": "env HARDHAT_NETWORK=mainnet yarn ts-node scripts/cvxcrv_optimal_apr.ts --holder",
    "solhint": "./node_modules/.bin/solhint -f table 'contracts/**/*.sol'",
    "prettier:solidity": "./node_modules/.bin/prettier --write 'contracts/**/*.sol'",
    "prettier": "./node_modules/.bin/prettier --write 'test/**/*.ts' 'scripts/**/*.ts' '*.ts' 'contracts/**/*.sol'",
    "coverage": "hardhat coverage",
    "eat:svg:mmd": "cd test/results && find . -name \"*.diagram.mmd\" -exec mmdc -i {} \\;",
    "eat:svg:gp": "cd test/results && find . -name \"*.gnuplot.gp\" -exec gnuplot -e \"set output '{}.svg'\" {} \\;",
    "eat:svg": "yarn eat:svg:mmd && yarn eat:svg:gp",
    "eat:png": "cd test/results && find . -name \"*.svg\" -exec convert {} {}.png \\;",
    "eat:run1": "ts-node test/eat_${0}.ts test/${0}${1}.config.yml && mmdc -i test/results/${0}${1}.blockchain.diagram.mmd",
    "eat1": "yarn eat:run1 ${0} ${1}",
    "eat": "yarn eat1 fxUSD  && yarn eat:svg && yarn eat:png"
  },
  "devDependencies": {
    "@mermaid-js/mermaid-cli": "^10.6.1",
    "@nomicfoundation/ethereumjs-block": "^5.0.3",
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.2",
    "@nomicfoundation/hardhat-ethers": "^3.0.4",
    "@nomicfoundation/hardhat-foundry": "^1.1.1",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.9",
    "@nomicfoundation/hardhat-verify": "^1.1.1",
    "@snapshot-labs/snapshot.js": "^0.4.71",
    "@typechain/ethers-v6": "^0.5.0",
    "@typechain/hardhat": "^9.0.0",
    "@types/asciichart": "^1.5.6",
    "@types/chai": "^4.2.21",
    "@types/crypto-js": "^4.2.2",
    "@types/edit-json-file": "^1.7.0",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash": "^4.14.202",
    "@types/mocha": "^9.0.0",
    "@types/node": "^12.0.0",
    "@types/yargs": "^17.0.32",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "asciichart": "^1.5.25",
    "axios": "^1.6.2",
    "chai": "^4.2.0",
    "commander": "^9.3.0",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.3.1",
    "dotenv-expand": "^10.0.0",
    "edit-json-file": "^1.7.0",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "ethers": "^6.9.0",
    "firebase": "^9.8.2",
    "hardhat": "^2.19.2",
    "hardhat-gas-reporter": "^1.0.4",
    "hardhat-preprocessor": "^0.1.5",
    "hardhat-waffle": "^0.0.1-security",
    "husky": "^8.0.1",
    "js-yaml": "^4.1.0",
    "lint-staged": "^13.0.3",
    "lodash": "^4.17.21",
    "prettier": "^2.3.2",
    "prettier-plugin-solidity": "^1.0.0-beta.13",
    "solhint": "^3.3.6",
    "solidity-coverage": "^0.8.5",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tty-table": "^4.1.6",
    "typechain": "^8.3.1",
    "typescript": "^5.2.2",
    "undici": "^5.22.1",
    "yargs": "^17.7.2"
  },
  "dependencies": {
    "@openzeppelin/contracts": "3.4.1-solc-0.7-2",
    "@openzeppelin/contracts-upgradeable": "3.4.1-solc-0.7-2"
  },
  "lint-staged": {
    "*.{js,ts}": "npx eslint --cache --fix",
    "*.sol": "prettier --ignore-unknown --write"
  },
  "engines": {
    "node": ">=10.4.0"
  },
  "packageManager": "yarn@4.1.0"
}
